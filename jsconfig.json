{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$src/*": ["src/*"],
      "$components/*": ["src/components/*"],
      "$data/*": ["data/*"]
    }
  },
  "include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
