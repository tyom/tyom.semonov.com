{
  "name": "tyom-semonov-cv",
  "description": "Tyom Semonov: CV",
  "type": "module",
  "private": true,
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit preview",
    "create-pdf": "node scripts/create-pdf.cjs",
    "predeploy": "rm -rf build && yarn create-pdf && yarn build && yarn build-storybook && cp -a storybook-static/. build/storybook && echo tyom.semonov.com > build/CNAME",
    "deploy": "gh-pages -d build --dotfiles",
    "lint": "eslint --ignore-path .gitignore . && prettier --check .",
    "format": "prettier --write .",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "is-plain-object": "^5.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.2.0",
    "popper.js": "1.16.1",
    "svelte-icons": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.4.0-beta.30",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.0-beta.30",
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "^1.0.0-next.193",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.9",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "3.2.3",
    "postcss": "^8.3.11",
    "postcss-load-config": "^3.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "puppeteer": "^11.0.0",
    "svelte": "^3.44.1",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.19",
    "tree-kill": "^1.2.2",
    "vite": "^2.6.13"
  }
}
