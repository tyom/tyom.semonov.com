{
  "name": "tyom-semonov.cv",
  "description": "Tyom Semonov: CV",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ignore-path=.gitignore",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "start": "node __sapper__/build",
    "export": "sapper build && ./bin/create-pdf && sapper export --legacy",
    "serve": "sirv __sapper__/export",
    "serve-dist": "sirv dist",
    "storybook": "start-storybook -p 6006 --ci --static-dir ./static",
    "build-storybook": "build-storybook -o dist/storybook --static-dir ./static",
    "prepare-deploy": "rm -rf dist/ && yarn export && yarn build-storybook && cp -r __sapper__/export/* dist/",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "is-plain-object": "^5.0.0",
    "js-yaml": "3.14.1",
    "markdown-it": "12.0.3",
    "polka": "^1.0.0-next.11",
    "popper.js": "1.16.1",
    "sirv": "^1.0.10",
    "svelte-icons": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@codechecks/build-size-watcher": "^0.1.0",
    "@codechecks/client": "^0.1.10",
    "@codechecks/lighthouse-keeper": "^0.1.2",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/svelte": "^6.1.10",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "eslint-config-tyom": "^2.6.0",
    "gh-pages": "3.1.0",
    "postcss": "^8.2.0",
    "postcss-import": "13.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "puppeteer": "5.5.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rucksack-css": "1.0.2",
    "sapper": "^0.28.10",
    "sirv-cli": "^1.0.10",
    "svelte": "^3.31.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "tree-kill": "^1.2.2"
  }
}
