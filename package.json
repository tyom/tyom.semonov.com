{
  "name": "tyom-semonov-cv",
  "description": "Tyom Semonov: CV",
  "type": "module",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit preview",
    "create-pdf": "node scripts/create-pdf.cjs",
    "predeploy": "rm -rf build && yarn create-pdf && yarn build && yarn build-storybook && cp -a storybook-static/. build/storybook && echo tyom.semonov.com > build/CNAME",
    "deploy": "gh-pages -d build --dotfiles",
    "lint": "eslint --ignore-path .gitignore . && prettier --check .",
    "format": "prettier --write .",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "is-plain-object": "^5.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.1",
    "popper.js": "1.16.1",
    "svelte-icons": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/svelte": "^6.5.9",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "^1.0.0-next.350",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "4.0.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^7.7.1",
    "prettier": "^2.7.0",
    "prettier-plugin-svelte": "^2.7.0",
    "puppeteer": "^14.4.0",
    "svelte": "^3.48.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.3",
    "tree-kill": "^1.2.2",
    "vite": "^2.9.12"
  }
}
