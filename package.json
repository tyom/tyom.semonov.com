{
  "name": "tyom-semonov-cv",
  "description": "Tyom Semonov: CV",
  "type": "module",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "create-pdf": "node scripts/create-pdf.cjs",
    "predeploy": "rm -rf build && yarn create-pdf && yarn build && yarn build-storybook && cp -a storybook/storybook-static/. build/storybook && echo tyom.semonov.com > build/CNAME",
    "deploy": "gh-pages -d build --dotfiles",
    "lint": "eslint --ignore-path .gitignore . && prettier --check .",
    "format": "prettier --write .",
    "storybook": "cd storybook && yarn start",
    "build-storybook": "cd storybook && yarn build",
    "postinstall": "yarn --cwd storybook"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "is-plain-object": "^5.0.0",
    "js-yaml": "^4.0.0",
    "markdown-it": "^12.0.4",
    "popper.js": "1.16.1",
    "svelte-icons": "^2.1.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/jit": "^0.1.18",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "gh-pages": "3.1.0",
    "postcss": "^8.2.9",
    "postcss-load-config": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "puppeteer": "^8.0.0",
    "svelte": "^3.37.0",
    "svelte-loader": "^3.1.0",
    "svelte-preprocess": "^4.7.0",
    "tailwindcss": "^2.1.0",
    "tree-kill": "^1.2.2",
    "vite": "^2.1.5"
  }
}
